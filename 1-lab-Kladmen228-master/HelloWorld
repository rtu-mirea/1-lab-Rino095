public class HelloWorld{
    public static void main(String[] args){
        System.out.println("pamagiti!!!");
        Student student = new Student();
        student.age = 18;
        student.name ="Danila";
        student.isAlive = false;
        System.out.println(student);
    }
}
class Student{
    public  String name;
    public int age;
    public boolean isAlive;
}